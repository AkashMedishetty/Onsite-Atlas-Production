# Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Backend Configuration (Required for 300+ participants)
NEXT_PUBLIC_BACKEND_URL=https://your-backend-url.railway.app
NEXT_PUBLIC_BACKEND_WS_URL=wss://your-backend-url.railway.app

# Backend Enforcement (NEW - Makes backend usage compulsory)
NEXT_PUBLIC_BACKEND_STRICT=true

# Optional: Development mode allows fallback
# NEXT_PUBLIC_BACKEND_STRICT=false

# Enterprise Backend Configuration
NEXT_PUBLIC_USE_ENTERPRISE_BACKEND=true

# Optional: Analytics and Monitoring
NEXT_PUBLIC_ANALYTICS_ID=your-analytics-id
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn

# Development Only
NEXT_PUBLIC_DEBUG=false 